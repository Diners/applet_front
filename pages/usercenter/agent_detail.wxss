}
.tabs_list image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  float: left;
}
.user_info{
  height: auto;
  float: left;
  margin-left: 26rpx;
}
.user_info text{
  width: 100%;
  height: auto;
  float: left;
  display: block;
}
.user_info text:nth-of-type(1){
