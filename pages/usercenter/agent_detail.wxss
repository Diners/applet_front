  margin-top: 30rpx;
}
.user_mobil text{
  color: #a4a4a4;
  font-size: 24rpx;
  float: left;
  display: block;
}
.user_mobil text:nth-of-type(1){
  margin-right: 40rpx;
}
.user_mobil text:nth-of-type(3){
  float: right;
}
.user_address{
  width: 100%;
