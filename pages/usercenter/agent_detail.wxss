/* pages/usercenter/agent_detail.wxss */
text{
  font-family: "微软雅黑";
}
.agent_box{
  width: 100%;
  height: auto;
  float: left;
  padding: 40rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.agent_box1{
  width: 100%;
  height: auto;
  float: left;
  padding: 40rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.agent_Content{
  width: 100%;
  height: auto;
  float: left;
  margin-top: 20rpx;
  /*background: white;*/
}
.agent_list{
  width: 100%;
  height: auto;
  float: left;
  position: relative;
}
.agent_list image:nth-of-type(1){
  width: 190rpx;
  height: 190rpx;
  float: left;
  border-radius: 8px;
  margin-right: 10rpx;
}
.agent_list_content{
  width: 65%;
  height: 100%;
  float: left;
  margin-left: 20rpx;
}
.agent_list_content text:nth-of-type(1){
  width: 100%;
  height: auto;
  float: left;
  display: block;
  margin-top:20rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #282828;
}
.agent_list_content text:nth-of-type(2){
  width: 100%;
  height: auto;
  color: #a4a4a4;
  font-size: 24rpx;
  float: left;
  display: block;
  margin-top: 20rpx;
}
.agent_list_content text:nth-of-type(3){
  color: #a4a4a4;
  font-size: 24rpx;
  float: left;
  display: block;
  margin-top: 20rpx;
}
.xuanze{
  width: 100%;
  height: 100rpx;
  float: left;
  padding-left: 23rpx;
  padding-right: 23rpx;
  box-sizing: border-box;
}
.fahuo_list{
  width: 100%;
  height: 80rpx;
  float: left;
  margin-top: 40rpx;
  border-radius: 50rpx;
  padding-left: 23rpx;
  padding-right: 23rpx;
  box-sizing: border-box;
  background: #ffd400;
  box-shadow:0px 20rpx 50rpx 2rpx rgba(255,221,0,0.2);
}
.fahuo_list text{
  width: 49%;
  height: 100%;
  float: left;
  box-sizing: border-box;
  text-align: center;
  line-height: 80rpx;
  font-size: 26rpx;
  color: white;
  
}
.he{
  width: 1px;
  height: 26rpx;
  float: left;
  border-left: 1px solid white;
  margin-top:26rpx; 
}
.active{
  width: 50%;
  height: 100%;
  float: left;
  color: black !important;
  box-sizing: border-box;
  text-align: center;
  line-height: 80rpx;
  font-size: 26rpx;
}
.active1{
  display: block;
}
.tabs{
  width: 100%;
