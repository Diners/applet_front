.toast{
  width: 100%;
  height: 124rpx;
  background: #ffd400;
  position: absolute;
  top: 0px;left:0px;
  padding: 30rpx;
  box-sizing: border-box;
  transition: 0.5s;
}
.toast_left{
  width: 80%;
  height: 100%;
  color: white;
  float: left;
  font-size: 28rpx;
  font-weight: bold;
  line-height: 35rpx;
}
.toast image{
  width: 40rpx;
  height: 40rpx;
  float: right;
}



.big_box{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  background: rgba(0,0,0,0.5);
}
.show_box{
  width: 604rpx;
  height: 247rpx;
  background: #ffd400;
  position: absolute;
  padding: 50rpx;
  box-sizing: border-box;
}
.show_box_content{
  width: 100%;
  height: auto;
  float: left;
}
.bot{
  font-size: 28rpx;
  font-weight: bold;
  color: white;
}
.show_box_content input{
  border-bottom: 1px solid white;
  padding-bottom: 30rpx;
  color: white;
}
.show_box_imgs{
  width: 100%;
  height: auto;
  float: left;
  margin-top: 50rpx;
}
.show_box_imgs image:nth-of-type(1){
  width: 30rpx;
  float: left;
}
.show_box_imgs image:nth-of-type(2){
  width: 50rpx;
  float: right;
}