text{
   font-family: "微软雅黑";
}
view{
   font-family: "微软雅黑";
}
.cartContainer{
  width: 100%;
  height: auto;
  float: left;
  background: #f6f6f6;
}
.content{
    width: 100%;
    padding: 40rpx;
    float: left;
    box-sizing: border-box;
    background: white;
   margin-bottom: 20rpx;
}
.content_title{
    width: 100%;
    height:60rpx;
    line-height: 60rpx;
    margin-bottom:16rpx;
}
.content_title .logo{
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.content_title view{
    font-size:28rpx;
    font-weight: bold;
    line-height: 25px;
    color: #ffdd00;
    float: left;
    margin-left: 20rpx;
}
.content_title .lead{
     width: 19rpx;
    height:31rpx;
    vertical-align: middle;
    float: right;
}
.goodOne{
    width: 100%;
    height:auto;
    float: left;
    padding-bottom: 40rpx;
    margin-top: 20rpx;
    border-bottom: 1px solid #f6f6f6;
    background: white;
    box-sizing: border-box;
}
.goodOne:nth-last-of-type(1){
  border: none;
}
.content_img{
    width: 190rpx;
    height:190rpx;
    float: left;
    border-radius: 8px;
    margin-top: 10rpx;
}
.content_font{
    width:65%;
    height:auto;
    float: left;
    margin-left: 40rpx;
    padding-top: 10px;
    box-sizing: border-box;
}
.content_font view{
    width: 100%;
    height:60rpx;
    line-height: 60rpx;
    float: left;
    overflow: hidden;
}
.content_font view:nth-of-type(1) view{
    width: 70%;
    font-size: 32rpx;
    font-weight: bold;
    color: #282828;
    float: left;
}
.content_font .close{
    width: 34rpx;
    height:34rpx;
    float: right;
}
.content_font .color{
    font-size:24rpx;
    color: #A4A4A4;
    margin-top: 10rpx;
}
.content_font .actions{
    margin-top:12rpx;
    height: 56rpx; 
    float: left;
    line-height: 56rpx;
}
/*.content_font .actions {
   font-size: 12px;
   color: #ff5a5f;
   float: left;
   width: 80rpx;
   height: 100%;
}*/
.price{
   font-size: 12px;
   color: #ff5a5f;
   float: left;
   width: 120rpx !important;
   height: 100%;
}
.content_font .actions .num_btn{
    width: 200rpx;
    height: 100%;
    float: right;
    line-height:56rpx;
    border-radius: 15px;
    margin: 0;
    padding: 0;
     border: 1px solid #f7f7f7;
}
.num_btn .add{
   width:40rpx;
   height: 100%;
   float: left;
   margin-left: 30rpx;
   line-height:56rpx;
    text-align: center;
    font-weight: bold;
    color: #aaa;
}
.num_btn .num{
  width: 70rpx;
  height: 100%;
  float: left;
  text-align: center;
}
.num_btn .jian{
    width: 20px;
    height: 100%;
    color: #282828;
    float: right; 
}
.bottom_flex{
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0px;
  left: 0px;
  background: white;
  z-index: 2;
  border: 1px solid #f6f6f6;
}
.bottom_flex>view:nth-of-type(1){
  width: 50%;
  height: 100%;
  float: left;
  line-height: 120rpx;
  padding-left: 40rpx;
  font-weight: bold;
  color: #282828;
  font-size:32rpx;
  font-family: "微软雅黑";
}
.bottom_flex>view text:nth-of-type(2){
  font-weight: normal !important;
  font-size: 24rpx !important;
  margin-left: 10rpx;
  color: #A4A4A4;
}
.Bst{
  width: 260rpx !important;
  height: 80rpx !important;
  float: right !important;
  margin-top: 20rpx;
  margin-right:40rpx !important; 
  border-radius: 45px;
  background: #ffdd00;
  text-align: center;
  line-height: 80rpx !important;
  color: white;
  font-size: 26rpx !important;
}
.quan{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  z-index: 3;
  background: rgba(0,0,0,0.4);
}
.error{
  width: 80%;
  height: 247rpx;
  background: red;
  position: absolute;
  top: 0px;
  left: 10%;
  padding: 50rpx;
  box-sizing: border-box;
}
.error>text{
  color: white;
  font-size: 32rpx;
  font-weight: bold;
}
.img_list{
  width: 100%;
  margin-top: 80rpx;
}
.img_list>image:nth-of-type(1){
  width: 30rpx;
}
.img_list>image:nth-of-type(2){
  width: 40rpx;
}
.img_list>image:nth-of-type(1){
  float: left;
}
.img_list>image:nth-of-type(2){
  float: right;
}
.fukuan{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  background: rgba(0,0,0,0.4);
  z-index: 4;
}
.pay_list{
  width: 100%;
  height: 543rpx;
  float: left;
  background:white;
  position: absolute;
  bottom: 0px;
  left: 0px;
  padding: 40rpx;
  box-sizing: border-box;
}
.pay_list>image:nth-of-type(1){
  width: 40rpx;
  float: right;
}
.pay_post{
  width: 100%;
  height: auto;
  float: left;
  margin-top: 80rpx;
}
.yue_list{
  width: 100%;
  height: auto;
  float: left;
  margin-bottom: 40rpx;
}
.yue_list>image:nth-of-type(1){
  width: 60rpx;
  float: left;
}
.yue_list>text:nth-of-type(1){
  height: 100%;
  display: block;
  float: left;
  margin-left: 26rpx;
  font-size: 26rpx;
  color: #282828;
  margin-top: 10rpx;
  font-weight: bold;
}
.yue_list_right{
  width: 30%;
  height: 100%;
  float: right;
}
.yue_list_right>text{
  font-size: 26rpx;
  color: #ff5a5f;
}
.yue_list_right>image{
  width: 40rpx;
  float: right;
  margin-top: 6rpx;
}
.btns{
  width: 100%;
  height: 100rpx;
  float: left;
  background: #ffdd00;
  text-align: center;
  line-height: 100rpx;
  color: white;
  font-size: 26rpx;
  margin-top: 30rpx;
}