.hide{
  display: none;
  transition: 2s linear;
  animation-delay:2s;
}
  .sure_mask{
           position: absolute;
            top:0;
            left: 0;
            width:100%;
            height:100%;
            background-color: rgba(0,0,0,.5);
            margin: 0;
            padding: 0;
            z-index:9999;
        }
        .sure_mask .sure_delete{
            background-color:#ff3500;
