            height: 124px;
        }
        .sure_mask .sure_delete .text{
            color: #fff;
            font-weight: bold;
            font-size: 14px;
            margin:0 25px;
            padding-top: 25px;
        }
       .sure_mask .sure_delete .del_btn, .sure_mask .sure_delete .sure_btn{
            display: inline-block;
