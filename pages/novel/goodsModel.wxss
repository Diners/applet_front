/* pages/novel/goodsModel.wxss */
page{
  position: relative;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}
text{
  font-family: "微软雅黑";
}
.User_box{
  width: 100%;
  height: 100%;
  position: fixed;
  background: #111;
  box-sizing: border-box;
}
.User_box>image:nth-of-type(1){
  width: 40rpx;
  position: absolute;
  top: 40rpx;
  left: 40rpx;
  z-index: 999;
}
.user_contents{
  width: 100%;
  height: auto;
  float: left;
  margin-top: 96rpx;
}
.user_blcak{
  width: 476rpx;
  height: auto !important;
  float: left;
  border: 2px solid black;
  margin-left: 80rpx;
  padding: 40rpx;
  z-index: 6;
}
.user_blcak text:nth-of-type(1){
  width: 80%;
  height: auto;
  line-height: 60rpx;
  display: block;
  font-size: 40rpx;
  color: #000000;
  font-weight: bold;
}
.user_blcak text:nth-of-type(2){
  margin-top: 20rpx;
  font-size: 28rpx;
}
.Big_img{
  width: 560rpx;
  height: 560rpx;
  float: right;
  margin-top: -160rpx;
  border: 1px solid red;
  z-index: 1;
}
.xinxibufen{
  width: 100%;
  height: auto;
  float: left;
  padding-right: 80rpx;
  box-sizing: border-box;
  margin-top: -20rpx;
}
.xinxibufen text:nth-of-type(1){
  width: 100%;
  height: auto;
  display: block;
  font-size: 32rpx;
  font-weight: bold;
  text-align: right;
  padding: 0px;
}
.xinxibufen text:nth-of-type(2){
  width: 160rpx;
  display: block;
  float: right;
  margin-top: 24rpx;
  border: 2px #000000 solid;
}
.headers{
  width: 100%;
  height: auto;
  float: right;
}
.headers image{
  width: 72rpx;
  height: 72rpx;
  float: right;
  margin-left: 20rpx;
  margin-top: 24rpx;
}
.headers text:nth-of-type(1){
  display: block;
  font-size: 26rpx;
  color: #000000;
  margin-top: 20rpx;
}
.headers text:nth-of-type(2){
  width: 80%;
  display: block;
  text-align: right;
  color: #a4a4a4;
  margin-top: 10rpx;
  font-size: 22rpx;
  font-style:oblique;
  border: none;
}
.user_cehua{
 height: 580rpx;
  width: 100%;
  position: fixed; 
  z-index: 999;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;
}
.user_cehua_top{
width: 580rpx;
height: auto;
float: left;
padding: 30rpx;
box-sizing: border-box;
}
.user_cehua_top>image{
  width: 117rpx;
  height: 117rpx;
  border-radius: 50%;
  float: left;
  margin-top: 30rpx;
  margin-left: 8rpx;
}
