  float: left;
  margin-top: 50rpx;
  margin-left: 30rpx;
}
.user_cehua_center>text{
  width: 100%;
  height: auto;
  display: block;
  float: left;
}
.user_cehua_center>text:nth-of-type(1){
  font-size: 32rpx;
  font-weight: bold;
  color: #ffffff;
}
.user_cehua_center>text:nth-of-type(2){
  color: #c8c8c8;
  font-size: 22rpx;
  margin-top: 18rpx;
}
.border_right{
  width: 1px;
  height: 40rpx;
  margin-top: 20rpx;
  float: left;
  border-right:1px solid white;
}
.user_xinxi{
  width: 100%;
  height: auto;
  float: left;
  margin-top: 60rpx;
}
.xinxi_one{
  width: auto;
  height: auto;
  float: left;
}

.xinxi_one>text{
  width: 100%;
  display: block;
}
.xinxi_one>text:nth-of-type(1){
  color: #c8c8c8;
  font-size: 22rpx;
  margin-left: 15rpx;
}
.xinxi_one>text:nth-of-type(2){
color: #ffd400;
font-size: 24rpx;
font-weight: bold;
margin-left: 15rpx;
margin-top: 20rpx;
}
.user_cehua_bottom{
  width: 580rpx;
  height: auto;
  background: white;
  float: left;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  box-sizing: border-box;
}
.user_list{
  width: 100%;
  height: 90rpx;
  float: left;
 padding-left: 40rpx;
 padding-right: 40rpx;
 box-sizing: border-box;
}
.user_list_active{
   width: 100%;
  height: 90rpx;
  float: left;
 padding-left: 40rpx;
 padding-right: 40rpx;
  box-sizing: border-box;
  background: #e7e7e7;
}
.user_list:nth-of-type(1){
  margin-top:13rpx;
}
.user_list>image{
  width: 40rpx;
  float: left;
  margin-top:20rpx; 
  margin-right: 57rpx;
}
.user_list>text{
  color: #000000;
  height: 100%;
  line-height: 90rpx;
  font-size: 28rpx;
}
.border_bottom{
  width: 100%;
  border-top: 1px solid #dddddd;
  float: left;
  margin-top: 19rpx;
}
.padding_bottom{
  width: 100%;
  float: left;
  background: white;
 padding-bottom: 60%;
}
