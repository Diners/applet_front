.Tips{
  width: 100%;
  height: 120rpx;
  position: absolute;
  top: 20rpx;
  left: 0px;
  padding-left: 40rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.Tips_son{
  width: 100%;
  height: 100%;
  background: #ffd400;
  padding: 30rpx;
  box-sizing: border-box;
}
.Tips_son text{
  width: 80%;
  height: 100%;
  display: block;
  float: left;
  color: white;
  font-size: 28rpx;
}
.Tips_son image{
  width: 40rpx;
